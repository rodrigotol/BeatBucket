{
  "name": "beatbucket",
  "version": "1.0.0",
  "description": "BeatBucket",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist-win": "electron-builder --win"
  },
  "build": {
    "productName": "BeatBucket",
    "icon": "assets/img/beat_bucket.ico",
    "fileAssociations": [
      {
        "ext": "mp3",
        "name": "BeatMp3"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/img/beat_bucket.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "author": {
    "name": "Rodrigo Silva"
  },
  "license": "MIT",
  "devDependencies": {
    "angular": "^1.7.8",
    "angular-animate": "^1.7.8",
    "angular-aria": "^1.7.8",
    "angular-material": "^1.1.19",
    "angular-messages": "^1.7.8",
    "angular-route": "^1.7.8",
    "angular-uuid": "0.0.4",
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "electron-is-dev": "^1.1.0",
    "howler": "^2.1.2",
    "sass": "^1.22.7"
  },
  "dependencies": {
    "angular": "^1.7.8",
    "angular-animate": "^1.7.8",
    "angular-aria": "^1.7.8",
    "angular-material": "^1.1.19",
    "angular-messages": "^1.7.8",
    "angular-route": "^1.7.8",
    "angular-uuid": "0.0.4",
    "electron-is-dev": "^1.1.0",
    "howler": "^2.1.2"    
   }
}
